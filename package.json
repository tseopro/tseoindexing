{
    "name": "tseoindexing",
    "version": "1.0.0",
    "devDependencies": {
        "@wordpress/scripts": "^26.12.0",
        "dir-archiver": "^2.1.0",
        "rtlcss": "^4.1.0"
    },
    "rtlcssConfig": {
      "options": {
        "autoRename": false,
        "autoRenameStrict": false,
        "blacklist": {},
        "clean": true,
        "greedy": false,
        "processUrls": false,
        "stringMap": []
      },
      "plugins": [],
      "map": false
    },
    "scripts": {
        "compile:rtl": "rtlcss public/css/tseoindexing.css public/css/tseoindexing-rtl.css",
        "style": "sass --watch sass/style.scss public/css/tseoindexing.css  --no-charset --no-source-map",
        "style-min": "sass --watch sass/style.scss public/css/tseoindexing.min.css  --no-charset --style=compressed --no-source-map",
        "admin": "sass --watch sass/admin.scss admin/css/tseoport.css  --no-charset --no-source-map",
        "admin-min": "sass --watch sass/admin.scss admin/css/tseoport.min.css  --no-charset --style=compressed --no-source-map",
        "bundle": "dir-archiver --src . --dest ../tseoindexing-v1.0.0.zip --exclude sass icons _psd .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore .vscode README.md composer.json composer.lock node_modules vendor package-lock.json package.json"
    },
    "dependencies": {
        "loader-utils": "^3.2.1",
        "sass": "^1.66.1"
    }
}